{
  "port": "env(PORT)",
  "settings": {
    "debug": true,
    "loggerFormat": "tiny",
    "session": {
      "store": "@foal/typeorm"
    },
    "jwt": {
      "privateKey": "env(JWT_PRIVATE_KEY)",
      "publicKey": "env(JWT_PUBLIC_KEY)"
    }
